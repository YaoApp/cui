{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"importHelpers": true,
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"sourceMap": true,
		"baseUrl": ".",
		"strict": true,
            "experimentalDecorators":true,
            "useDefineForClassFields":true,
		"paths": {
			"@/*": ["*"],
			"@@/*": [".umi/*"]
		},
		"allowSyntheticDefaultImports": true
	}
}

{
      "type":"Page",
      "children": {
            "type": "Table",
            "config": {
                  "primary":"id",
                  "apis": {
                        "setting": "/setting/table/service",
                        "search": "/table/service/search",
                        "save": "/table/service/save"
                  },
                  "items":"@fileds.service",
                  "header":{
                        "title": "云服务",
                        "actions":{
                              "batch": {
                                    "apis": {
                                          "delete": "/xiang/table/service/delete/in",
                                          "update": "/xiang/table/service/update/in"
                                    },
                                    "columns": [
                                          {
                                                "name": "服务名称",
                                                "width": 12
                                          },
                                          {
                                                "name": "所属厂商",
                                                "width": 12
                                          },
                                          {
                                                "name": "服务类型",
                                                "width": 12
                                          },
                                          {
                                                "name": "状态",
                                                "width": 12
                                          },
                                          {
                                                "name": "服务领域",
                                                "width": 24
                                          }
                                    ],
                                    "delete": true
                              },
                              "import": {
                                    "apis": {
                                          "setting": "/xiang/import/order/setting",
                                          "mapping": "/xiang/import/order/mapping",
                                          "mapping_setting": "/xiang/import/order/mapping/setting",
                                          "preview": "/xiang/import/order/data",
                                          "preview_setting": "/xiang/import/order/data/setting",
                                          "import": "/xiang/import/order"
                                    }
                              }
                        }
                  },
                  "filter": {
                        "children":[
                              {
                                    "name": "关键词",
                                    "width": 4,
                                    "bind": "where.name.like",
                                    "input": {
                                          "type": "input",
                                          "props": {
                                                "placeholder": "请输入关键词"
                                          }
                                    }
                              }
                        ],
                        "actions": {
                              "create": {
                                    "text": "新建服务",
                                    "icon":"icon-plus"
                              }
                        }
                  },
                  "list":{
                        "hideView": true,
                        "hideEdit": false,
                        "unfold": true,
                        "columns":[
                              {
                                    "name": "服务信息",
                                    "width": 240
                              },
                              {
                                    "name": "所属厂商"
                              },
                              {
                                    "name": "服务类型"
                              },
                              {
                                    "name": "标签多选"
                              },
                              {
                                    "name": "状态"
                              },
                              {
                                    "name": "服务领域"
                              },
                              {
                                    "name": "计费方式"
                              },
                              {
                                    "name": "创建时间"
                              }
                        ],
                        "actions":{
                              "checkbox": [
                                    {
                                          "value": ":status",
                                          "visible_label": false,
                                          "status": [
                                                {
                                                      "label": "开启",
                                                      "value": "enabled"
                                                },
                                                {
                                                      "label": "关闭",
                                                      "value": "disabled"
                                                }
                                          ]
                                    }
                              ],
                              "items": [
                                    {
                                          "useModal": true,
                                          "type": "view",
                                          "title": "查看-Modal",
                                          "icon": "icon-eye",
                                          "formName": "menu",
                                          "formId": ":id"
                                    }
                              ]
                        }
                  },
                  "pagination":{
                        "showTotal": true
                  }
            },
            "props": {
                  "scroll": {"x": 1200}
            }
      }
}
{
      "name": "xgen",
      "version": "1.0.0",
      "author": "Wendao",
      "description": "Next-generation low-code dashboard.",
      "private": false,
      "license": "Apache-2.0",
      "homepage": "https://yaoapps.com",
      "packageManager": "pnpm@6.32.2",
      "repository": {
            "type": "git",
            "url": "https://github.com/YaoApp/xgen"
      },
      "scripts": {
            "dev": "umi dev",
            "build": "umi build",
            "build:all": "turbo run build",
            "build:com": "turbo run build --scope=@yaoapp/components",
            "build:utils": "turbo run build --scope=@yaoapp/utils",
            "postinstall": "umi setup"
      },
      "gitHooks": {
            "pre-commit": "lint-staged --allow-empty"
      },
      "lint-staged": {
            "*.{js,jsx,less,md,json}": [
                  "prettier --write"
            ],
            "*.ts?(x)": [
                  "prettier --parser=typescript --write"
            ]
      },
      "dependencies": {
            "@lcdp/offline-plugin": "^5.1.1",
            "@yaoapp/components": "workspace:*",
            "@yaoapp/utils": "workspace:*",
            "ahooks": "^3.1.10",
            "antd": "^4.18.9",
            "clsx": "^1.1.1",
            "echarts": "^5.3.0",
            "lodash-es": "^4.17.21",
            "mobx-react-lite": "^3.3.0",
            "moment": "^2.29.1",
            "mustache": "^4.2.0",
            "nprogress": "^0.2.0",
            "query-string": "^7.1.1",
            "react": "^17.0.2",
            "react-countup": "^6.1.1",
            "react-dom": "^17.0.2",
            "react-helmet-async": "^1.2.3",
            "react-slick": "^0.28.1",
            "store": "^2.0.12",
            "umi": "^4.0.0-rc.3"
      },
      "devDependencies": {
            "@parcel/packager-ts": "2.3.2",
            "@parcel/transformer-less": "2.3.2",
            "@parcel/transformer-typescript-types": "2.3.2",
            "@swc/core": "^1.2.146",
            "@types/lodash-es": "^4.17.6",
            "@types/mustache": "^4.1.2",
            "@types/nprogress": "^0.2.0",
            "@types/react": "^17.0.39",
            "@types/react-dom": "^17.0.11",
            "@types/react-slick": "^0.23.8",
            "@types/store": "^2.0.2",
            "less-vars-to-js": "^1.3.0",
            "lint-staged": "^12.3.4",
            "postcss": "^8.4.5",
            "prettier": "^2.5.1",
            "turbo": "^1.1.4",
            "typescript": "^4.6.2",
            "yorkie": "^2.0.0"
      }
}
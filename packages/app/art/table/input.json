{
      "template": "table",
      "@title": "云服务",
      "@pageExtraActions": [
            {
                  "title": "导入",
                  "icon": "icon-import",
                  "action": {
                        "id": "$table",
                        "method": "showImportModal"
                  }
            },
            {
                  "title": "投屏显示",
                  "icon": "icon-airplay",
                  "action": {
                        "id": "$page",
                        "method": "history.push",
                        "params": {
                              "pathname": "/screen/service"
                        }
                  }
            }
      ],
      "@tableActionsConfig": {
            "batch": {
                  "columns": [
                        {
                              "name": "服务名称",
                              "width": 12
                        },
                        {
                              "name": "所属厂商",
                              "width": 12
                        },
                        {
                              "name": "服务类型",
                              "width": 12
                        },
                        {
                              "name": "状态",
                              "width": 12
                        },
                        {
                              "name": "服务领域",
                              "width": 24
                        }
                  ],
                  "delete": true
            },
            "import": {
                  "bind": "order"
            }
      },
      "@filterChildren": [
            {
                  "name": "关键词",
                  "width": 4,
                  "bind": "where.name.like",
                  "input": {
                        "type": "input",
                        "props": {
                              "placeholder": "请输入关键词"
                        }
                  }
            }
      ],
      "@filterCreateTitle": "服务",
      "@list": {
            "columns": [
                  {
                        "name": "服务信息",
                        "width": 240
                  },
                  {
                        "name": "所属厂商"
                  },
                  {
                        "name": "服务类型"
                  },
                  {
                        "name": "标签多选"
                  },
                  {
                        "name": "状态"
                  },
                  {
                        "name": "服务领域"
                  },
                  {
                        "name": "计费方式"
                  },
                  {
                        "name": "创建时间"
                  }
            ],
            "actions": {
                  "checkbox": [
                        {
                              "value": ":status",
                              "visible_label": false,
                              "status": [
                                    {
                                          "label": "开启",
                                          "value": "enabled"
                                    },
                                    {
                                          "label": "关闭",
                                          "value": "disabled"
                                    }
                              ]
                        }
                  ],
                  "items": [
                        {
                              "useModal": true,
                              "type": "view",
                              "title": "查看-Modal",
                              "icon": "icon-eye",
                              "formName": "menu",
                              "formId": ":id"
                        }
                  ]
            }
      },
      "@tableProps": {
            "scroll": {
                  "x": 1200
            }
      }
}